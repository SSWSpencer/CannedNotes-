{
    "manifest_version": 3,
 
    "name": "CannedNotes+", 
    "description": "Create custom canned notes",
    "version": "0.1.0",
    "author": "Steven Spencer",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "CannedNotes+"
    },
    "permissions": ["storage"]
}